@model SUP_INV1._0.ViewModel.ViewModel1
<h2>Future Demand Predictions</h2>
<table class="table">
    <thead>
        <tr>

            <th>Forecasted Amount</th>
            <th>Upper Bound</th>
            <th>Lower Bound</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var prediction in Model.FPForcastResults)
        {
            <tr>

                <td>@prediction.PredictedAmount</td>
                <td>@prediction.UpperBounds</td>
                <td>@prediction.LowerBounds</td>
            </tr>
        }
    </tbody>
</table>


