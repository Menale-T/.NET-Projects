@model SUP_INV1._0.ViewModel.ViewModel1

<h2>Prediction Results</h2>

<table border="1">
    <thead>
        <tr>
            <th>Predicted Amount</th>
            <th>Lower Bound</th>
            <th>Upper Bound</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.FPForcastResults)
        {
            <tr>
                <td>@item.PredictedAmount</td>
                <td>@item.LowerBounds</td>
                <td>@item.UpperBounds</td>
            </tr>
        }
    </tbody>
</table>
