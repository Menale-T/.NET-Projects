@model IEnumerable<SUP_INV1._0.Models.PREPAREDRECORD>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ItemID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecSoldAmountQuantity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecSt_InPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecSt_OutPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecDateSold)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecSupplierEmail)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ItemID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecSoldAmountQuantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecSt_InPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecSt_OutPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecDateSold)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecSupplierEmail)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RecID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RecID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RecID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
