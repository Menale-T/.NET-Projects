@model IEnumerable<SUP_INV1._0.Models.FarmProducts>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Amount_In_Kg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Maximum_Shelf_Life_Days)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stock_In_Price_Per_Kg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stock_Out_Price_Per_Kg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date_Created)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Supplier_Email)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount_In_Kg)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Maximum_Shelf_Life_Days)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stock_In_Price_Per_Kg)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stock_Out_Price_Per_Kg)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date_Created)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Supplier_Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
